{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es3",
        "moduleResolution": "node",
        "declaration": true,
        "inlineSources": true,
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "outDir": "js/cjs"
    },
    "exclude": [
        "node_modules",
        "js"
    ]
}
